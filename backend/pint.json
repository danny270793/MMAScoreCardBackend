{
    "preset": "laravel",
    "rules": {
        "ordered_class_elements": {
            "order": [
                "use_trait",
                "constant_public",
                "constant_protected",
                "constant_private",
                "property_public",
                "property_protected",
                "property_private",
                "method_public",
                "method_protected",
                "method_private"
            ]
        }
    }
}
