{
  "name": "io.github.danny270793.ufcevents",
  "displayName": "UFCEvents",
  "version": "1.0.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "build:android": "cordova build android",
    "start:android": "cordova run android",
    "build:ios": "cordova build ios",
    "start:ios": "xcrun simctl shutdown all && cordova run ios --target=iPhone-16-Pro"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-statusbar": {}
    }
  },
  "devDependencies": {
    "cordova-android": "^13.0.0",
    "cordova-ios": "^7.1.1",
    "cordova-plugin-statusbar": "^4.0.0"
  }
}